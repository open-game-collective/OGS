{
  "name": "strikers-game-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/strikers-game-web/src",
  "targets": {
    "build": {
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "executor": "@nxtensions/astro:build",
      "options": {}
    },
    "dev": {
      "executor": "@nxtensions/astro:dev",
      "options": {}
    },
    "start": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "strikers-game-web:build",
        "buildTargetOptions": {
          "outputFileName": "dist/apps/strikers-game-web/dist/server/entry.mjs"
        }
      },
      "configurations": {
        "production": {
          "buildTarget": "strikers-game-web:build:production"
        }
      }
    },
    "preview": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "executor": "@nxtensions/astro:preview",
      "options": {}
    },
    "check": {
      "executor": "@nxtensions/astro:check"
    },
    "sync": {
      "executor": "@nxtensions/astro:sync"
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/strikers-game-web/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/strikers-game-web",
        "configDir": "apps/strikers-game-web/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "serve-storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "host": "0.0.0.0",
        "buildTarget": "strikers-game-web:build-storybook",
        "parallel": false,
        "staticFilePath": "dist/storybook/strikers-game-web"
      },
      "configurations": {
        "ci": {
          "buildTarget": "strikers-game-web:build-storybook:ci"
        }
      }
    }
  },
  "tags": []
}
